/* pages/component/auth/auth.wxss */
.accredit{
    width:100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 99999;
    display: flex;
    align-items: center;
    justify-content: center;
}
.accredit-view{
    width: 400rpx;
    height: 400rpx;
    background: #fff;
    border: 2rpx solid #eee;
    border-radius: 10rpx;
    position: relative;
}
.accredit-btn{
    
    color: #fff;
    width:80%; 
    font-size: 32rpx;
    line-height: 80rpx;
    position: absolute;
    left: 10%;
    bottom: 20rpx;
}
